
public class StringProgram {
	
	
	public static String maxchar(String s1)
	{
		String s="";
	for(int i=0; i< s1.length(); i++)
    {
		
		
		if(s1.charAt(i)==s1.charAt(i+1))
		{
			
		}
    }
    return s;
	}
	// String programs::
	public static void main(String[] args)
	{
		/*String s1 = "Java";
	    String s= "";*/
	    maxchar("JAVA");
	}

}
