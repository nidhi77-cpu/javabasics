
public class StringProgram {
	
	
	public static int maxchar(String s1)
	{
	
		String s ="";
		int count =0;
		for(int i=0; i<s1.length();i++)
		{
			if(s1.charAt(i)=='A')
			{
			 count++;
			}
		}
		return count;
		
		
	}
	
	public static void main(String[] args)
	{
		/*String s1 = "Java";
	    String s= "";*/
	    maxchar("JAVA");
	   
	}

}
