
public class LeetCOdeProblemsonArrays {

	public static void main(String[] args) {
		int[] nums = { 1, 2, 1 };
		getConcatenation(nums);

	}

	private static int [] getConcatenation(int[] nums) {
		
        int [] sol = new int [2* nums.length];
		for (int i = 0; i < sol.length; i++) {
			sol[i] = nums[i];
		}
		
       return sol;
	}

}
